{
  "name": "asset-validator",
  "displayName": "Asset Validator",
  "description": "An extension that checks if all assets declared in pubspec.yaml are correctly placed in the assets folder and whether they are being used in the app.",
  "version": "1.0.0",
  "publisher": "hassine-wassef",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.94.0"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/Hwassef/assets-validator.git"
},
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "assetValidator.validate",
        "title": "Validate Flutter Assets"
      }
      ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^1.0.00",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "yaml": "^2.6.0"
  }
}
